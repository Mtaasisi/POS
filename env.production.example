# Production Environment Variables for Clean App
# Copy this file to .env.production and fill in your actual values

# Supabase Configuration
VITE_SUPABASE_URL=https://your-project.supabase.co
VITE_SUPABASE_ANON_KEY=your-anon-key-here

# App Configuration
VITE_APP_NAME=Clean App
VITE_APP_VERSION=1.0.0
VITE_APP_ENV=production

# Feature Flags
VITE_ENABLE_PWA=true
VITE_ENABLE_OFFLINE=true
VITE_ENABLE_NOTIFICATIONS=true

# API Configuration
VITE_API_TIMEOUT=30000
VITE_MAX_FILE_SIZE=10485760

# SMS Configuration (if using SMS features)
VITE_SMS_ENABLED=true
VITE_SMS_PROVIDER=mobishastra
VITE_SMS_API_URL=https://mshastra.com/sendurl.aspx
VITE_SMS_USERNAME=your-username
VITE_SMS_PASSWORD=your-password
VITE_SMS_SENDER_ID=INAUZWA

# Email Configuration (if using email features)
VITE_EMAIL_ENABLED=true
VITE_EMAIL_PROVIDER=sendgrid
VITE_EMAIL_API_KEY=your-sendgrid-key

# Analytics (optional)
VITE_ANALYTICS_ENABLED=false
VITE_GOOGLE_ANALYTICS_ID=

# Debug Mode (set to false in production)
VITE_DEBUG_MODE=false 